{
    "name": "leearvin/php-mongo-db",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "mongo",
        "swoole"
    ],
    "authors": [
        {
            "name": "Arvin Hermit",
            "email": "arvin.hermit@gmail.com",
            "homepage": "https://github.com/Arvin-Lee"
        }
    ],
    "description": "php mongo db lib.",
    "autoload": {
        "psr-4": {
            "Php\\MongoDb\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "psr/container": "^1.0",
        "mongodb/mongodb": "^1.5"
    },
    "require-dev": {
    },
    "suggest": {
        "hyperf/di": "required hyperf/di ~2.0.0 OR required hyperf/di ~1.1.0",
        "hyperf/pool": "required hyperf/pool ~2.0.0 OR required hyperf/pool ~1.1.0",
        "hyperf/cache": "required hyperf/cache ~2.0.0 OR required hyperf/cache ~1.1.0",
        "hyperf/utils": "required hyperf/utils ~2.0.0 OR required hyperf/utils ~1.1.0",
        "hyperf/contract": "required hyperf/contract ~2.0.0 OR required hyperf/contract ~1.1.0",
        "hyperf/command": "required hyperf/command ~2.0.0 OR required hyperf/command ~1.1.0",
        "hyperf/config": "required hyperf/config ~2.0.0 OR required hyperf/config ~1.1.0",
        "hyperf/task": "required hyperf/config ~2.0.0 OR required hyperf/task ~1.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Php\\MongoDb\\ConfigProvider"
        }
    }
}
